<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">\
        <shape>
            <corners
                android:radius="25dp"
                ></corners>

            <stroke
                android:color="@color/holo_purple"
                android:width="2dp"
                ></stroke>

            <solid
                android:color="@color/holo_blue_bright"
                ></solid>
        </shape>

    </item>

    <item >
        <shape>
            <corners android:radius="25dp"></corners>
            <stroke
                android:color="@color/holo_purple"
                android:width="2dp"
                ></stroke>
            <solid
                android:color="@color/holo_blue_light"
                ></solid>

        </shape>
    </item>

</selector>